<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SiluetaYa.Views.PrimeraFase.Page3"
             NavigationPage.HasNavigationBar="false"
             BackgroundColor="#5D810A">
    <StackLayout>
        <Image Source="Logo.png" WidthRequest="150" HorizontalOptions="CenterAndExpand" VerticalOptions="Start" Margin="20,50"></Image>
        <Button CornerRadius="30" HeightRequest="60" WidthRequest="60" BorderWidth="1" TranslationY="2" Text="INICIA YA" 
                BackgroundColor="#95C323" Clicked="Button_Clicked"></Button>
        <Button CornerRadius="30" HeightRequest="60" WidthRequest="60" BorderWidth="1" TranslationY="60" Text="VENDE YA" 
                BackgroundColor="#F7A015" ></Button>
    </StackLayout>
</ContentPage>